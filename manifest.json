{
  "manifest_version": 3,
  "name": "Testudo",
  "version": "0.1",
  "description": "Verberg ongezond nieuws op nos.nl",
  "icons": {
    "48": "icons/icon-48.png"
  },
  "content_scripts": [
    {
      "matches": ["*://nos.nl/*"],
      "js": ["content.js"],
      "run_at": "document_end"
    }
  ],
  "options_ui": {
    "page": "options.html",
    "open_in_tab": true
  }
}
